SetEnv PRODUCTION 1

<IfModule mod_rewrite.c>

RewriteEngine On

RewriteRule uploads(/|$) index.php [L]

RewriteRule js/[0-9]*/(.*) js/$1
RewriteRule css/([^.]*)\.[0-9]*\.css css/$1.css

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule . /index.php [L]

</IfModule>

AddType video/ogg  .ogv
AddType video/mp4  .mp4
AddType video/webm .webm

SetEnvIfNoCase Request_URI \.(og[gv]|mp4|m4v|webm)$ no-gzip dont-vary

# Cache

<FilesMatch "\.(ico|svg|woff|woff2|eot|ttf|jpg|png|gif|css|js|mp4|webm)$">
	Header set Cache-Control "max-age=31536000, public"
</FilesMatch>

<IfModule mod_headers.c>
    Header set Cache-Control "public, must-revalidate, proxy-revalidate, no-transform"
    Header unset Server
</IfModule>

